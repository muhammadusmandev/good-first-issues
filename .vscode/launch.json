{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "gfi:debugger",
            "type": "debugpy",
            "request": "launch",
            "program": "${workspaceFolder}/venv/bin/gfi",
            "args": ["search","facebook"],
            "console": "integratedTerminal",
            "python": "${workspaceFolder}/venv/bin/python",
            "cwd": "${workspaceFolder}",
            "env": {
                "PYTHONPATH": "${workspaceFolder}"
            }
        }
    ]
}
